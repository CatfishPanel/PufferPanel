{
  "name": "PufferPanel",
  "description": "Reinventing the way game servers are managed.",
  "version": "0.9.0",
  "contributors": [],
  "devDependencies": {
    "chai": "3.3.*",
    "coveralls": "2.11.4",
    "eslint": "1.5.*",
    "eslint-plugin-hapi": "1.2.0",
    "istanbul": "0.3.21",
    "mocha": "2.3.*",
    "mocha-lcov-reporter": "1.0.0"
  },
  "dependencies": {
    "async": "1.4.2",
    "bcrypt": "0.8.5",
    "boom": "^2.9.0",
    "colors": "1.1.2",
    "crumb": "4.0.*",
    "date-utils": "1.2.17",
    "fs-extra": "0.24.*",
    "h2o2": "4.0.*",
    "hapi": "10.1.*",
    "hapi-auth-cookie": "3.1.0",
    "hapi-locals": "0.1.7",
    "hoek": "2.16.*",
    "i18n": "0.5.0",
    "inert": "3.0.1",
    "joi": "6.7.*",
    "mailcomposer": "1.1.0",
    "mailgun": "0.5.0",
    "moment": "^2.10.6",
    "nodejs-nocaptcha-recaptcha": "^1.0.0",
    "nodemailer": "1.5.0",
    "nodemailer-smtp-transport": "1.0.3",
    "notp": "2.0.3",
    "nunjucks": "2.1.*",
    "path": "0.12.*",
    "postmark": "1.2.1",
    "randomstring": "1.1.*",
    "recaptcha": "^1.2.1",
    "rethinkdbdash": "2.1.*",
    "rfr": "1.2.*",
    "sendgrid": "1.9.2",
    "thirty-two": "0.0.2",
    "underscore": "1.8.3",
    "url-parse": "1.0.3",
    "vision": "3.0.0",
    "winston": "1.0.2",
    "wreck": "6.2.0",
    "yar": "4.2.*",
    "yargs": "3.26.*"
  },
  "scripts": {
    "coveralls": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec ./tests/*.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test-eslint": "eslint --plugin hapi lib/* index.js tests/*",
    "test-mocha": "mocha tests --redirectConsole=false",
    "test": "sh scripts/execute-tests.sh",
    "start": "node index.js",
    "start-verbose": "node index.js --consoleLevel=verbose",
    "start-debug": "node index.js --consoleLevel=debug --showMeta=true"
  },
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ]
    }
  },
  "bugs": {
    "url": "https://github.com/PufferPanel/PufferPanel/issues"
  },
  "repository": [
    {
      "type": "git",
      "url": "https://github.com/PufferPanel/PufferPanel.git"
    }
  ],
  "license": "GPL-3.0",
  "homepage": "http://pufferpanel.com"
}
